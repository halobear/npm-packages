{
  "name": "@halobear/vue-editor",
  "version": "0.0.4",
  "description": "一个洁面简介的vue编辑器，支持富文本和markdown",
  "keywords": [
    "vue",
    "markdown-editor",
    "rich-editor"
  ],
  "scripts": {
    "dev": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/test.config.js yarn serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:lib": "vue build index.js -t lib -d lib -n vue-editor",
    "lint": "vue-cli-service lint",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 --commit-path '.'"
  },
  "author": "luzhongkuan <luzhongk@126.com>",
  "homepage": "https://github.com/halobear/npm-packages/tree/master/packages/vue-editor",
  "license": "ISC",
  "main": "lib/vue-editor.umd.min.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halobear/npm-packages.git"
  },
  "bugs": {
    "url": "https://github.com/halobear/npm-packages/issues"
  },
  "dependencies": {
    "core-js": "3.4.4",
    "vue": "^2.6.10"
  }
}
