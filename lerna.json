{
  "version": "1.0.4",
  "packages": [
    "packages/*"
  ],
  "command": {
    "publish": {
      "ignoreChanges": [
        "docs",
        "*.md"
      ],
      "message": "chore(release): publish"
    },
    "bootstrap": {
      "ignore": "ignore/*",
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  }
}
